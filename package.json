{
  "name": "muiscon",
  "version": "3.0.0-alpha.2",
  "description": "Music Player using react, redux, nodejs and mongodb",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "server": "export JWT_SECRET=somestring && node ./bin/www",
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReuvenLiran/react-musicplayer"
  },
  "author": "Liran Rueven",
  "dependencies": {
    "axios": "^0.15.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.18.0",
    "better-npm-run": "0.0.11",
    "browser-id3-writer": "^2.3.1",
    "classnames": "^2.2.5",
    "cookie-parser": "~1.3.5",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "debug": "^2.3.3",
    "ejs": "^2.4.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.24.1",
    "ip": "^1.1.4",
    "mime-types": "^2.1.13",
    "mongodb": "^2.2.15",
    "morgan": "~1.6.1",
    "node-sass": "^3.13.0",
    "normalize.css": "^4.1.1",
    "postcss-loader": "^0.13.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-autosuggest": "^8.0.0",
    "react-dom": "^15.4.1",
    "react-motion": "^0.4.7",
    "react-redux": "^4.4.6",
    "react-router": "^2.8.0",
    "react-swipeable-views": "^0.8.3",
    "react-tap-event-plugin": "^2.0.1",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-form": "^4.1.3",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "serve-favicon": "^2.3.2",
    "shuffle-array": "^1.0.0",
    "style-loader": "^0.13.1",
